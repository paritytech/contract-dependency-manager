{
    "name": "@dotdm/cdm",
    "version": "0.3.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "browser": {
                "types": "./dist/browser.d.ts",
                "import": "./dist/browser.js"
            },
            "import": "./dist/index.js"
        },
        "./codegen": {
            "types": "./dist/codegen.d.ts",
            "import": "./dist/codegen.js"
        }
    },
    "files": ["dist"],
    "publishConfig": {
