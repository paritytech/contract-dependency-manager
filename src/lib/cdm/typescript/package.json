    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@dotdm/contracts": "workspace:*",
        "@dotdm/env": "workspace:*",
        "@dotdm/utils": "workspace:*",
        "@polkadot-api/sdk-ink": "catalog:",
        "polkadot-api": "catalog:"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "typescript": "catalog:"
    }
}
