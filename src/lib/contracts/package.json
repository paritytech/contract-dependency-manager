        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@dotdm/env": "workspace:*",
        "@dotdm/utils": "workspace:*",
        "@noble/hashes": "catalog:",
        "@polkadot-api/sdk-ink": "catalog:",
        "multiformats": "catalog:",
        "polkadot-api": "catalog:"
    },
    "peerDependencies": {
        "@polkadot-api/descriptors": "*"
    },
    "peerDependenciesMeta": {
        "@polkadot-api/descriptors": {
            "optional": true
        }
    },
    "devDependencies": {
        "@polkadot-api/descriptors": "workspace:*",
        "@types/node": "catalog:",
        "typescript": "catalog:"
    }
