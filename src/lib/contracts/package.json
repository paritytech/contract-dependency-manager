{
    "name": "@dotdm/contracts",
    "version": "0.1.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": ["dist"],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
